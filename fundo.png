                    required: function () {
                        return isTipoOrgaoPrivado();
                    }
                },
                segmento: {
                    required: function () {
                        return isTipoOrgaoPrivado();
                    }
                },
                cf_cargo_privado: {
                    required: function () {
                        return isTipoOrgaoPrivado();
                    }
                },
                numero_de_funcionarios: {
                    required: function () {
                        return isTipoOrgaoPrivado();
                    }
                }
            },
            messages: {
                nome: "Insira seu nome",
                email: "Insira um email válido",
                whatsapp: "Insira seu WhatsApp",
                tipo_orgao: "Selecione uma opção",
