            // Função para obter valor do parâmetro da URL
            function getUrlParameter(name) {
                const url = window.location.search;
                const regex = new RegExp('[?&]' + name + '=([^&#]*)');
                const results = regex.exec(url);
                return results ? decodeURIComponent(results[1].replace(/\+/g, ' ')) : null;
            }

            // Captura os parâmetros utm_content e utm_medium
            const utmCampaign = getUrlParameter('utm_campaign');
            const utmMedium = getUrlParameter('utm_medium');

            //Variável do objeto que será guardado
            var obj;

            obj = {
                email: jQuery(".form-newsletter-a .wpcf7-email").val(),
                page_url: jQuery(location).attr('href'),
                page_name: jQuery(location).attr('pathname')
            };

            function formv3() {
                // Create the new request 
                var xhr = new XMLHttpRequest();
                var url = 'https://api.hsforms.com/submissions/v3/integration/submit/22638114/4d8d30f8-646f-4a9d-9120-b3345ddc3baf'
                function getCookie(name) {
                    let cookie = {};
                    document.cookie.split(';').forEach(function (el) {
                        let [k, v] = el.split('=');
                        cookie[k.trim()] = v;
                    })
                    return cookie[name];
                }
                const hubspotutk = getCookie("hubspotutk");

                // request JSON:
                var data = {
                    "fields": [
                        {
                            "objectTypeId": "0-1",
                            "name": "email",
                            "value": obj.email
                        },
                        { "objectTypeId": "0-1", "name": "slug", "value": "post-blog" },
						{ "objectTypeId": "0-1", "name": "slug_historico", "value": "post-blog" },
                    ],
                    "context": {
                        "hutk": hubspotutk,
                        "pageUri": obj.page_url,
                        "pageName": obj.page_name
                    }
                }
                if ( utmCampaign ) {
                        data.fields.push({ "objectTypeId": "0-1", "name": "utm_campaign_email", "value": utmCampaign });
											  data.fields.push({ "objectTypeId": "0-1", "name": "utm_campaign", "value": utmCampaign });
                    }
                if (utmMedium) {
                    data.fields.push({ "objectTypeId": "0-1", "name": "utm_medium", "value": utmMedium });
                }

                var final_data = JSON.stringify(data)

                xhr.open('POST', url);
                // Sets the value of the 'Content-Type' HTTP request headers to 'application/json'
                xhr.setRequestHeader('Content-Type', 'application/json');
                xhr.onreadystatechange = function () {
                    if (xhr.readyState == 4 && xhr.status == 200) {
                        console.log(xhr.responseText); // Returns a 200 response if the submission is successful.
                        jQuery(".form-newsletter-a #btn-submit").trigger('click');
