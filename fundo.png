            mask: ["(99) 9999-9999", "(99) 99999-9999"],
            keepStatic: true
        });
        jQuery("[name='tipo_orgao']").change(function () {
            var selectedValue = jQuery("[name='tipo_orgao']").val(); // Pega o valor do option selecionado
            if (selectedValue === "Sim") {
                jQuery('.inputs-publico').show();
                jQuery('.inputs-privado').hide(); // Esconde o outro grupo de inputs
            } else if (selectedValue === "Não") {
                jQuery('.inputs-privado').show();
                jQuery('.inputs-publico').hide(); // Esconde o outro grupo de inputs
            }
        });

        jQuery("input.wpcf7-tel").on('focusout', function (event) {
            var target, phone, element;
            target = (event.currentTarget) ? event.currentTarget : event.srcElement;
            phone = target.value.replace(/\D/g, '');
            element = jQuery(target);

            // Atualiza a máscara de acordo com o comprimento do telefone
            if (phone.length > 10) {
                element.inputmask("setvalue", phone);
            } else {
                element.inputmask("setvalue", phone);
            }
        });

        jQuery(".form-popup-pdf #btn-validation").on("click", function (event) {
            event.preventDefault(); // Evita o comportamento padrão do botão
            if (jQuery(".form-popup-pdf form.wpcf7-form").valid()) {
                // Obtendo o form id utilizando o pathname
                var page_name = jQuery(location).attr('pathname');
                page_name = page_name.replace(/^\/|\/$/g, '');
                // Variável do objeto que será guardado
                var obj = {
                    firstname: jQuery("[name='nome']").val(),
                    email: jQuery("#email_id").val(),
                    phone: jQuery("[name='whatsapp']").val(),
                    tipoOrgao: jQuery("[name='tipo_orgao']").val(),
                    tipo_de_organizacao: jQuery("[name='tipo_de_organizacao'] option:selected").val(),
                    empresaPrivada: jQuery("[name='empresa']").val(),
                    cf_setor: jQuery("[name='cf_setor']").val(),
                    segmentoPrivado: jQuery("[name='segmento']").val(),
                    nFuncionarios: jQuery("[name='numero_de_funcionarios']").val(),
                    cf_cargo_privado: jQuery("[name='cf_cargo_privado']").val(),
                    cf_cargo_publico: jQuery("[name='cf_cargo_publico']").val(),
                    cf_id_estado: jQuery("#id_uf option:selected").text(),
                    cidade_id: jQuery("#id_cidade option:selected").val(),
                    id_cidade: jQuery("#id_cidade option:selected").text(),
                    page_url: jQuery(location).attr('href')
                };

                // Atribuir valor ao input de nome da pagina
                jQuery("[name='urlPagina']").val(obj.page_url);

                function formv3() {
                    // Create the new request 
                    var xhr = new XMLHttpRequest();
                    var url = 'https://api.hsforms.com/submissions/v3/integration/submit/22638114/866d349d-e6f6-4053-8065-394e64519e65';
                    function getCookie(name) {
                        let cookie = {};
                        document.cookie.split(';').forEach(function (el) {
                            let [k, v] = el.split('=');
                            cookie[k.trim()] = v;
                        })
                        return cookie[name];
                    }
                    const hubspotutk = getCookie("hubspotutk");

                    // request JSON:
                    var data = {
                        "fields": [
                            { "objectTypeId": "0-1", "name": "firstname", "value": obj.firstname },
                            { "objectTypeId": "0-1", "name": "email", "value": obj.email },
                            { "objectTypeId": "0-1", "name": "phone", "value": obj.phone },
                            { "objectTypeId": "0-1", "name": "e_orgao_publico___contato_", "value": obj.tipoOrgao },
                            { "objectTypeId": "0-1", "name": "tipo_de_formulario", "value": "Blog" },
                            { "objectTypeId": "0-1", "name": "slug", "value": page_name },
                            { "objectTypeId": "0-1", "name": "slug_historico", "value": page_name }
                        ],
                        "context": {
                            "hutk": hubspotutk,
                            "pageUri": obj.page_url,
                            "pageName": page_name
                        }
                    };
                    if (jQuery("[name='tipo_orgao']").val() === "Sim") {
                        data.fields.push({ "objectTypeId": "0-1", "name": "tipo_de_organizacao", "value": obj.tipo_de_organizacao });
                        data.fields.push({ "objectTypeId": "0-1", "name": "cf_cargo_nome", "value": obj.cf_cargo_publico });
                        data.fields.push({ "objectTypeId": "0-1", "name": "cf_setor", "value": obj.cf_setor });
                        data.fields.push({ "objectTypeId": "0-1", "name": "cf_id_estado", "value": obj.cf_id_estado });
                        data.fields.push({ "objectTypeId": "0-1", "name": "cidade_id", "value": obj.cidade_id });
                        data.fields.push({ "objectTypeId": "0-1", "name": "id_cidade", "value": obj.id_cidade });

                    }
                    if (jQuery("[name='tipo_orgao']").val() === "Não") {
                        data.fields.push({ "objectTypeId": "0-1", "name": "company", "value": obj.empresaPrivada });
                        data.fields.push({ "objectTypeId": "0-1", "name": "cf_cargo_nome", "value": obj.cf_cargo_privado });
                        data.fields.push({ "objectTypeId": "0-1", "name": "segmento", "value": obj.segmentoPrivado });
                        data.fields.push({ "objectTypeId": "0-1", "name": "numero_de_funcionarios", "value": obj.nFuncionarios });
                    }
                    if (utmCampaign) {
                        data.fields.push({ "objectTypeId": "0-1", "name": "utm_campaign", "value": utmCampaign });
                    }
                    if (utmMedium) {
                        data.fields.push({ "objectTypeId": "0-1", "name": "utm_medium", "value": utmMedium });
                    
                        if (utmMedium === 'email') {
                            data.fields.push({ "objectTypeId": "0-1", "name": "utm_campaign_email", "value": utmCampaign });
                            data.fields.push({ "objectTypeId": "0-1", "name": "tipo_de_formulario___campanhas_de_e_mail", "value": 'Material Rico' });
                        }
                    
                        if (typeof utmMedium === 'string' && utmMedium.includes('whatsapp')) {
                            data.fields.push({ "objectTypeId": "0-1", "name": "utm_campaign_whatsapp", "value": utmCampaign });
                            data.fields.push({ "objectTypeId": "0-1", "name": "tipo_de_formulario___campanha_de_whatsapp", "value": 'Material Rico' });
                        }
                    
                        if (typeof utmMedium === 'string' && utmMedium.includes('sequencia')) {
                            data.fields.push({ "objectTypeId": "0-1", "name": "utm_campaign_sequencia", "value": utmCampaign });
                            data.fields.push({ "objectTypeId": "0-1", "name": "tipo_de_formulario___sequencias", "value": 'Material Rico' });
                        }
                    }
                    var final_data = JSON.stringify(data);
                    xhr.open('POST', url);
                    xhr.setRequestHeader('Content-Type', 'application/json');

                    xhr.onreadystatechange = function () {
                        if (xhr.readyState == 4) {
                            if (xhr.status == 200) {
                                console.log(xhr.responseText); // Returns a 200 response if the submission is successful.
                                // Redirecionar página após envio do form

                                var url = $(".pgfw-single-pdf-download-button").attr('href');
                                function openInNewTab(url) {
                                    window.open(url, '_blank').focus();
                                }
                                openInNewTab(url);

                            } else if (xhr.status == 400) {
                                console.log(xhr.responseText); // Returns a 400 error the submission is rejected.
                            } else if (xhr.status == 403) {
                                console.log(xhr.responseText); // Returns a 403 error if the portal isn't allowed to post submissions.
                            } else if (xhr.status == 404) {
                                console.log(xhr.responseText); // Returns a 404 error if the formGuid isn't found
                            }
                        }
                    };

                    xhr.send(final_data);
                }
                formv3();
            } else {
                console.log("Formulário invalido")
            }
        });

        jQuery("#id_uf").change(function () {
